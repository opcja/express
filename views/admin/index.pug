extends ../layout

block content
  h1= title
  nav   
    ul 
     li
      a(href="/admin/news/add") Dodaj artykuł
  table
    tr 
      th id 
      th tytuł
      th Data dodania
      th akcja
    each item, index in data
      tr
        td=index
        td=item.title 
        td=item.created
        td 
          a(href=`/admin/news/delete/${item.id}`) Usuń 


  
